{
  "version": "1.0.0",
  "description": "Hackatalk",
  "repository": "https://github.com/dooboolab/hackatalk/server",
  "license": "MIT",
  "scripts": {
    "start": "dotenv -e ./dotenv/.env -- ts-node --transpile-only --files src/server.ts",
    "start:prod": "dotenv -e ./dotenv/prod.env -- ts-node --transpile-only --files src/server.ts",
    "dev": "dotenv -e ./dotenv/.env -- ts-node-dev --respawn --transpile-only --files src/server.ts",
    "prod": "node dist/src/server",
    "clean": "rm -rf dist",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src tests",
    "build": "npm -s run clean && npm -s run generate && tsc && cp -r files dist/ && cp -r html dist/",
    "tsc": "tsc",
    "test": "dotenv -e ./dotenv/test.env -- jest --runInBand --detectOpenHandles --forceExit",
    "generate": "yarn generate:prisma && yarn generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "generate:local": "dotenv -e ./dotenv/.env -- yarn generate:prisma && dotenv -e ./dotenv/.env -- yarn generate:nexus",
    "generate:test": "npm -s run generate:prisma:test && npm -s run generate:nexus:test",
    "generate:prisma:test": "dotenv -e ./dotenv/test.env -- prisma generate",
    "generate:nexus:test": "dotenv -e ./dotenv/test.env  -- ts-node --transpile-only src/schema",
    "test:watch": "jest --watch",
    "migrate": "prisma migrate deploy",
    "migrate:reset": "dotenv -e ./dotenv/.env -- prisma migrate reset",
    "migrate:dev": "dotenv -e ./dotenv/.env -- prisma migrate dev",
    "migrate:deploy": "dotenv -e ./dotenv/.env -- prisma migrate deploy",
    "db-push:local": "dotenv -e ./dotenv/.env -- prisma db push",
    "db-push:test": "dotenv -e ./dotenv/test.env -- prisma db push",
    "studio": "dotenv -e ./dotenv/.env -- prisma studio",
    "docker:build": "docker build --tag hackatalk/server .",
    "docker:start": "docker run --name hackatalk -e 'NODE_ENV=production' -it -p 4000:4000 -d hackatalk/server",
    "docker:remove": "docker stop hackatalk && docker rm $(docker ps -a -q) && docker rmi hackatalk/server",
    "docker:push": "docker tag hackatalk/server dooboolab/hackatalk_server && docker push dooboolab/hackatalk_server",
    "docker:remote": "docker run --name hackatalk -e 'NODE_ENV=production' -it -p 4000:4000 -d dooboolab/hackatalk_server",
    "docker:logs": "docker logs -f hackatalk"
  },
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@azure/storage-blob": "^12.11.0",
    "@prisma/client": "^4.4.0",
    "@sendgrid/mail": "^7.7.0",
    "apollo-server": "^3.10.2",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "ejs": "^3.1.8",
    "generate-password": "^1.7.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-middleware": "^6.1.32",
    "graphql-redis-subscriptions": "^2.5.0",
    "graphql-relay": "^0.10.0",
    "graphql-shield": "^7.6.2",
    "graphql-subscriptions": "^2.0.0",
    "graphql-upload": "^13.0.0",
    "graphql-ws": "^5.11.2",
    "helmet": "^6.0.0",
    "i18next": "^21.9.2",
    "i18next-http-middleware": "^3.2.1",
    "i18next-node-fs-backend": "^2.1.3",
    "ioredis": "^5.2.3",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.4",
    "nanoid": "^3.3.4",
    "nexus": "^1.3.0",
    "node-rsa": "^1.1.1",
    "ramda": "^0.28.0",
    "stream-mime-type": "^1.0.2",
    "verify-apple-id-token": "^3.0.0",
    "ws": "^8.9.0"
  },
  "devDependencies": {
    "@dooboo/eslint-config-node": "^1.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/ejs": "^3.1.1",
    "@types/express": "^4.17.14",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.1.1",
    "@types/jsonwebtoken": "8.5.9",
    "@types/multer": "^1.4.7",
    "@types/nanoid": "^3.0.0",
    "@types/node": "^18.7.23",
    "@types/node-rsa": "^1.1.1",
    "@types/ramda": "^0.28.15",
    "@types/ws": "8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "casual": "^1.6.2",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.24.0",
    "eslint-plugin-jest": "^27.0.4",
    "eslint-plugin-prettier": "^4.2.1",
    "graphql-import": "^1.0.2",
    "graphql-request": "^5.0.0",
    "graphql-tag": "^2.12.6",
    "jest": "29.1.2",
    "node-fetch": "^3.2.10",
    "prettier": "^2.7.1",
    "prisma": "^4.4.0",
    "ts-jest": "^29.0.3",
    "ts-node": "10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "resolutions": {
    "fs-capacitor": "^6.2.0",
    "graphql-upload": "^13.0.0"
  }
}
