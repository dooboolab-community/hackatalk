{
  "version": "1.0.0",
  "description": "Hackatalk",
  "repository": "https://github.com/dooboolab/hackatalk/server",
  "license": "MIT",
  "scripts": {
    "local": "dotenv -e ./dotenv/.env -- ts-node-dev --respawn --transpile-only --files src/server.ts",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "start": "ts-node src/server.ts",
    "prod": "node dist/server",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "lint": "eslint src/**/**/*.ts && eslint tests/**/**/*.ts",
    "tsc": "tsc",
    "test": "dotenv -e ./dotenv/test.env -- jest --runInBand --detectOpenHandles --forceExit",
    "generate:test": "npm -s run generate:prisma:test && npm -s run generate:nexus:test",
    "generate:prisma:test": "dotenv -e ./dotenv/test.env -- prisma generate",
    "generate:nexus:test": "dotenv -e ./dotenv/test.env  -- ts-node --transpile-only src/schema",
    "test:watch": "jest --watch",
    "generate": "yarn generate:prisma && yarn generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "postinstall": "npm -s run generate",
    "migrate": "prisma migrate deploy --preview-feature",
    "migrate:reset": "dotenv -e ./dotenv/prod.env -- prisma migrate reset --preview-feature",
    "migrate:dev": "dotenv -e ./dotenv/prod.env -- prisma migrate dev --preview-feature",
    "migrate:deploy": "dotenv -e ./dotenv/prod.env -- prisma migrate deploy --preview-feature",
    "db-push:local": "dotenv -e ./dotenv/.env -- prisma db push --preview-feature",
    "db-push:test": "dotenv -e ./dotenv/test.env -- prisma db push --preview-feature",
    "studio": "dotenv -e ./dotenv/.env -- prisma studio"
  },
  "dependencies": {
    "@nexus/schema": "^0.19.2",
    "@prisma/client": "2.13.0",
    "@sendgrid/mail": "^7.4.0",
    "apollo-server": "^2.19.0",
    "axios": "^0.21.0",
    "azure-storage": "^2.10.3",
    "bcrypt": "^5.0.0",
    "ejs": "^3.1.5",
    "generate-password": "^1.5.1",
    "graphql-iso-date": "^3.6.1",
    "graphql-middleware": "^4.0.2",
    "graphql-relay": "^0.6.0",
    "graphql-shield": "^7.4.3",
    "graphql-subscriptions": "^1.1.0",
    "i18next": "^19.8.4",
    "i18next-http-middleware": "^3.0.6",
    "i18next-node-fs-backend": "^2.1.3",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "nexus": "^0.26.1",
    "nexus-plugin-prisma": "^0.26.0",
    "node-rsa": "^1.1.1",
    "ramda": "^0.27.1",
    "verify-apple-id-token": "^2.0.1"
  },
  "devDependencies": {
    "@prisma/cli": "~2.13.0",
    "@types/bcrypt": "^3.0.0",
    "@types/ejs": "^3.0.5",
    "@types/express": "^4.17.9",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/jest": "^26.0.19",
    "@types/jsonwebtoken": "8.5.0",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.12",
    "@types/node-rsa": "^1.0.0",
    "@types/ramda": "^0.27.32",
    "@types/ws": "7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-datasource-rest": "^0.9.5",
    "apollo-link-ws": "^1.0.20",
    "apollo-server-testing": "^2.19.0",
    "casual": "^1.6.2",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.15.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.1.0",
    "graphql-import": "^1.0.2",
    "graphql-request": "^3.3.0",
    "graphql-tag": "^2.11.0",
    "jest": "26.6.3",
    "node-fetch": "^2.6.1",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "ts-node": "9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
