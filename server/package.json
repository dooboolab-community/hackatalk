{
  "version": "1.0.0",
  "description": "Hackatalk",
  "repository": "https://github.com/dooboolab/hackatalk/server",
  "license": "MIT",
  "scripts": {
    "local": "dotenv -e ./dotenv/.env -- nodemon --exec ts-node src/server.ts",
    "dev": "ts-node-dev --no-notify --respawn --transpileOnly src/server.ts",
    "start": "ts-node src/server.ts",
    "prod": "node dist/server",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "lint": "eslint src/**/**/*.ts && eslint tests/**/**/*.ts",
    "tsc": "tsc",
    "test": "dotenv -e ./dotenv/test.env -- jest --runInBand --detectOpenHandles --forceExit",
    "generate:test": "npm -s run generate:prisma:test && npm -s run generate:nexus:test",
    "generate:prisma:test": "dotenv -e ./dotenv/test.env -- prisma generate",
    "generate:nexus:test": "dotenv -e ./dotenv/test.env  -- ts-node --transpile-only src/schema",
    "test:watch": "jest --watch",
    "generate": "yarn generate:prisma && yarn generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "postinstall": "npm -s run generate",
    "migrate:init": "rm -rf prisma/migrations && prisma migrate save --name “init” --experimental",
    "migrate:save": "dotenv -e ./dotenv/.env -- prisma migrate save --experimental",
    "migrate:up": "dotenv -e ./dotenv/.env -- prisma migrate up --experimental",
    "migrate:down": "dotenv -e ./dotenv/.env -- prisma migrate down 1 --experimental",
    "migrate:dev": "dotenv -e ./dotenv/dev.env -- prisma migrate up --experimental",
    "migrate:test": "dotenv -e ./dotenv/test.env -- prisma migrate up --experimental",
    "studio": "dotenv -e ./dotenv/.env -- prisma studio --experimental"
  },
  "dependencies": {
    "@nexus/schema": "^0.14.0",
    "@prisma/client": "^2.1.3",
    "@sendgrid/mail": "^7.2.1",
    "apollo-server": "^2.15.1",
    "azure-storage": "^2.10.3",
    "bcrypt-nodejs": "^0.0.3",
    "bcryptjs": "^2.4.3",
    "ejs": "^3.1.3",
    "generate-password": "^1.5.1",
    "graphql-iso-date": "^3.6.1",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.3.2",
    "graphql-subscriptions": "^1.1.0",
    "i18next": "^19.5.4",
    "i18next-express-middleware": "^2.0.0",
    "i18next-node-fs-backend": "^2.1.3",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "nexus": "^0.25.0",
    "nexus-plugin-prisma": "^0.16.1",
    "virgil-crypto": "^4.2.2",
    "virgil-sdk": "^6.1.2"
  },
  "devDependencies": {
    "@prisma/cli": "2.1.3",
    "@types/bcrypt-nodejs": "^0.0.31",
    "@types/bcryptjs": "2.4.2",
    "@types/ejs": "^3.0.4",
    "@types/express": "^4.17.6",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/jest": "^26.0.3",
    "@types/jsonwebtoken": "8.5.0",
    "@types/multer": "^1.4.3",
    "@types/node": "^14.0.14",
    "@types/ws": "7.2.6",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-ws": "^1.0.20",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^3.2.0",
    "eslint": "^7.4.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "graphql-request": "^2.0.0",
    "graphql-tag": "^2.10.3",
    "jest": "26.1.0",
    "node-fetch": "^2.6.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.1",
    "ts-node": "8.10.2",
    "ts-node-dev": "1.0.0-pre.50",
    "typescript": "^3.9.6"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
