{
  "name": "hackatalk",
  "private": true,
  "version": "0.0.0",
  "description": "Opensource chat app",
  "author": "dooboolab",
  "license": "GPL-3.0-only",
  "postinstall": "npx jetify",
  "scripts": {
    "dev": "node node_modules/react-native/local-cli/cli.js start",
    "tsc": "tsc",
    "cp:config": "cp .env.sample .env && cp .env.sample .env.prod",
    "watch": "tsc -w",
    "start": "npm run dev",
    "codecov": "codecov",
    "upgrade": "react-native-git-upgrade && npm install --save react-native",
    "upgrade:major": "react-native upgrade",
    "test": "jest",
    "web": "expo start --web",
    "start:packager": "node_modules/react-native/packager/packager.sh",
    "start:reset": "node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "android:shake": "adb shell input keyevent 82",
    "android:log": "adb logcat '*:S' ReactNative:V ReactNativeJS:V",
    "android:apk": "cd android && ENVFILE=.env.prod ./gradlew assembleRelease",
    "android:release": "ENVFILE=.env.prod node node_modules/react-native/local-cli/cli.js run-android --variant=release",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "ios:release": "node node_modules/react-native/local-cli/cli.js run-ios --configuration Release",
    "ios:devices": "xcrun simctl list devices",
    "ios:dooboolab": "node node_modules/react-native/local-cli/cli.js run-ios --device 'dooboolab'",
    "ios:dooboolab-release": "node node_modules/react-native/local-cli/cli.js run-ios --configuration Release --device 'dooboolab'",
    "ios:iphone5": "node node_modules/react-native/local-cli/cli.js run-ios  --simulator='iPhone 5s'",
    "ios:iphone7": "node node_modules/react-native/local-cli/cli.js run-ios  --simulator='iPhone 7'",
    "ios:reset": "rm -rf ios/build/ModuleCache",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "build:ios": "ENVFILE=.env.prod react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ./ios",
    "build:android": "ENVFILE=.env.prod react-native bundle --entry-file index.js --platform android --dev false --verbose --bundle-output android/main.jsbundle --assets-dest ./android",
    "relay": "relay-compiler --src ./src --schema schema.graphql --language typescript --artifactDirectory ./src/__generated__",
    "relay-watch": "yarn relay-compiler --schema schema.graphql --src ./src/ --extensions ts tsx --language typescript --artifactDirectory ./src/__generated__ --watch",
    "schema": "get-graphql-schema https://hackatalk.azurewebsites.net/graphql > schema.graphql",
    "codegen": "graphql-codegen --config codegen.yml",
    "generate": "yarn schema && yarn codegen"
  },
  "dependencies": {
    "@dooboo-ui/native": "^0.6.3",
    "@dooboo-ui/native-pinch-zoom-slider-modal": "^0.0.5",
    "@dooboo-ui/native-theme": "^0.1.7",
    "@expo/react-native-action-sheet": "^3.8.0",
    "@expo/vector-icons": "^10.2.0",
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/core": "^5.8.2",
    "@react-navigation/material-top-tabs": "^5.2.8",
    "@react-navigation/native": "^5.4.3",
    "@react-navigation/stack": "^5.4.0",
    "@virgilsecurity/e3kit-native": "^2.3.1",
    "@virgilsecurity/key-storage-rn": "^1.0.0",
    "expo": "~37.0.3",
    "expo-apple-authentication": "^2.1.1",
    "expo-asset": "~8.1.5",
    "expo-constants": "^9.0.0",
    "expo-device": "^2.1.0",
    "expo-facebook": "~8.1.0",
    "expo-font": "^8.1.1",
    "expo-google-sign-in": "~8.1.0",
    "expo-image-manipulator": "^8.1.0",
    "expo-image-picker": "~8.1.0",
    "expo-linear-gradient": "~8.1.0",
    "expo-localization": "~8.1.0",
    "expo-permissions": "^8.1.0",
    "expo-updates": "~0.2.0",
    "graphql": "^15.1.0",
    "i18n-js": "^3.5.1",
    "immer": "^6.0.8",
    "moment": "^2.26.0",
    "react": "^0.0.0-experimental-33c3af284",
    "react-dom": "^0.0.0-experimental-33c3af284",
    "react-native": "^0.62.2",
    "react-native-appearance": "~0.3.4",
    "react-native-bootsplash": "^2.2.4",
    "react-native-config": "^1.1.0",
    "react-native-gesture-handler": "~1.6.1",
    "react-native-image-pan-zoom": "^2.1.12",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-keychain": "^6.0.0",
    "react-native-localize": "^1.4.0",
    "react-native-modalbox": "^2.0.0",
    "react-native-onesignal": "^3.7.3",
    "react-native-reanimated": "~1.9.0",
    "react-native-redash": "^14.1.1",
    "react-native-safe-area-context": "2.0.2",
    "react-native-screens": "2.8.0",
    "react-native-svg": "^12.1.0",
    "react-native-tab-view": "^2.14.2",
    "react-native-unimodules": "^0.9.1",
    "react-native-virgil-crypto": "^0.6.0",
    "react-native-web": "~0.11.7",
    "react-native-webview": "^9.4.0",
    "react-relay": "^0.0.0-experimental-94e87455",
    "relay-hooks": "^3.4.0",
    "sentry-expo": "^2.0.4",
    "styled-components": "^5.1.1",
    "subscriptions-transport-ws": "^0.9.16",
    "unimodules-font-interface": "^5.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-transform-runtime": "7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/runtime": "^7.9.6",
    "@bam.tech/react-native-make": "^3.0.0",
    "@dooboo/eslint-config": "^0.4.2",
    "@graphql-codegen/cli": "^1.15.0",
    "@graphql-codegen/core": "^1.15.0",
    "@graphql-codegen/typescript": "^1.15.0",
    "@testing-library/jest-native": "^3.1.0",
    "@testing-library/react-hooks": "^3.2.1",
    "@testing-library/react-native": "^5.0.3",
    "@types/i18n-js": "^3.0.3",
    "@types/jest": "^25.2.3",
    "@types/react": "^16.9.35",
    "@types/react-native": "^0.62.11",
    "@types/react-native-modalbox": "^1.4.8",
    "@types/react-relay": "^7.0.8",
    "@types/react-test-renderer": "^16.9",
    "@types/relay-test-utils": "^6.0.2",
    "@types/styled-components": "^5.1.0",
    "abort-controller": "^3.0.0",
    "babel-jest": "^25.5.1",
    "babel-plugin-relay": "^9.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-preset-expo": "^8.2.1",
    "babel-preset-react-native": "^5.0.2",
    "codecov": "^3.7.0",
    "eslint": "^7.1.0",
    "get-graphql-schema": "^2.1.2",
    "import-sort-style-eslint": "^6.0.0",
    "jest": "^25.5.1",
    "jest-expo": "^37.0.0",
    "jest-expo-ts": "^22.0.1",
    "jest-fetch-mock": "^3.0.3",
    "jest-styled-components": "^7.0.2",
    "jetifier": "^1.6.5",
    "metro-react-native-babel-preset": "^0.59.0",
    "prettier": "^2.0.5",
    "prettier-plugin-import-sort": "^0.0.4",
    "react-native-svg-transformer": "^0.14.3",
    "react-test-renderer": "^16.13.1",
    "relay-compiler": "^9.1.0",
    "relay-compiler-language-typescript": "^12.0.2",
    "relay-test-utils": "^9.1.0",
    "ts-jest": "^25.5.1",
    "typescript": "3.9.3"
  },
  "importSort": {
    ".js, .jsx": {
      "style": "eslint",
      "options": {}
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "eslint",
      "options": {}
    }
  }
}
