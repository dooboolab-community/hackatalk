    "@graphql-tools/relay-operation-optimizer": "6.3.0",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/react-hooks": "^5.1.2",
    "@testing-library/react-native": "^8.0.0-rc.0",
    "@types/apollo-upload-client": "^14.1.0",
    "@types/expo": "^33.0.1",
    "@types/i18n-js": "^3.8.2",
    "@types/jest": "^26.0.24",
    "@types/mime": "^2.0.3",
    "@types/react": "~17.0.13",
    "@types/react-native": "~0.63.2",
    "@types/react-native-modalbox": "^1.4.8",
    "@types/react-relay": "^11.0.1",
    "@types/react-responsive": "^8.0.2",
    "@types/react-test-renderer": "^17.0",
    "@types/relay-test-utils": "^6.0.4",
    "abort-controller": "^3.0.0",
    "babel-plugin-inline-dotenv": "^1.6.0",
    "babel-plugin-relay": "^11.0.2",
    "babel-preset-expo": "8.4.0",
    "codecov": "^3.8.2",
    "eslint": "^7.30.0",
    "eslint-plugin-i18n-json": "^3.1.0",
    "get-graphql-schema": "^2.1.2",
    "import-sort-style-eslint": "^6.0.0",
    "jest": "^27.0.6",
    "jest-expo": "^42.0.0",
    "jest-fetch-mock": "^3.0.3",
    "jetifier": "^2.0.0",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.3.2",
    "prettier-plugin-import-sort": "^0.0.7",
    "react-dom": "16.13.1",
    "react-native-svg-transformer": "^0.14.3",
    "react-test-renderer": "^16.11.0",
    "relay-compiler": "^11.0.2",
    "relay-compiler-language-typescript": "^14.0.0",
    "relay-config": "^11.0.2",
    "relay-test-utils": "^11.0.2",
    "sharp-cli": "^1.15.0",
    "ts-jest": "^27.0.3",
    "typescript": "~4.0.0"
  },
  "importSort": {
    ".js, .jsx": {
      "style": "eslint",
      "options": {}
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "eslint",
      "options": {}
    }
  }
}
