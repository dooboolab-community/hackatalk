    "get-graphql-schema": "^2.1.2",
    "import-sort-style-eslint": "^6.0.0",
    "jest": "^27.5.1",
    "jest-expo": "^45.0.1",
    "jest-fetch-mock": "^3.0.3",
    "jetifier": "^2.0.0",
    "metro-babel-register": "^0.71.0",
    "mockdate": "^3.0.5",
    "patch-package": "^6.4.7",
