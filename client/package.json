    "react-test-renderer": "17.0.2",
    "relay-compiler": "^13.2.0",
    "relay-compiler-language-typescript": "^15.0.1",
    "relay-config": "^12.0.1",
    "relay-test-utils": "^13.2.0",
    "ts-jest": "^28.0.4",
    "typescript": "~4.7.2"
  },
  "importSort": {
    ".js, .jsx": {
      "style": "eslint",
      "options": {}
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "eslint",
      "options": {}
    }
  },
  "resolutions": {
    "react-native/@jest/create-cache-key-function": "^27.0.2",
    "@types/react": "17.0.40",
    "@types/react-native": "^0.67.6"
  }
