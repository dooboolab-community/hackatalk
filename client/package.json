    "expo-updates": "~0.13.2",
    "expo-video-thumbnails": "~6.3.0",
    "expo-web-browser": "~10.2.1",
    "graphql": "^16.5.0",
    "graphql-ws": "^5.8.2",
    "i18n-js": "^3.9.2",
