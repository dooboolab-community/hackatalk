    "@babel/runtime": "^7.14.6",
    "@dooboo/eslint-config": "^0.8.1",
    "@emotion/babel-plugin": "^11.3.0",
    "@expo/webpack-config": "~0.12.63",
    "@graphql-codegen/cli": "^1.21.4",
    "@graphql-codegen/typescript": "^1.22.0",
