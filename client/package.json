    "@react-navigation/material-top-tabs": "^6.2.4",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/stack": "^6.3.1",
    "apollo-upload-client": "^17.0.0",
    "dooboo-ui": "^0.1.97",
