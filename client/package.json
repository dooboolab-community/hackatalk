    "react": "18.1.0",
    "react-native": "0.70.5",
    "react-native-error-boundary": "^1.1.16",
    "react-native-gesture-handler": "~2.8.0",
    "react-native-get-random-values": "~1.8.0",
    "react-native-modalbox": "^2.0.2",
