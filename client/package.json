    "abort-controller": "^3.0.0",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "babel-plugin-relay": "^13.2.0",
    "babel-preset-expo": "~9.1.0",
    "codecov": "^3.8.3",
    "eslint": "^8.16.0",
