    "expo-image-picker": "~10.2.2",
    "expo-linear-gradient": "~9.2.0",
    "expo-localization": "~10.2.0",
    "expo-notifications": "~0.12.3",
    "expo-random": "~11.2.0",
    "expo-screen-orientation": "~3.2.1",
    "expo-splash-screen": "~0.11.2",
    "expo-updates": "~0.8.0",
    "expo-web-browser": "~9.2.0",
    "graphql": "^15.5.1",
    "i18n-js": "^3.8.0",
    "immer": "^9.0.5",
